{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:event:allow-listen",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://thunderstore.io/*"
        }
      ]
    }
  ]
}